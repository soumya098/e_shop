{
	"compilerOptions": {
		"target": "ES2016",
		"lib": ["dom", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noEmit": true,
		"jsx": "react-jsx"
	}
}
